#compdef create-tca-client

# Zsh completion for create-tca-client
# Creates a TCA client dependency with Interface, Models, Mocks, and Live implementation

_create-tca-client() {
  local curcontext="$curcontext" state line
  typeset -A opt_args

  _arguments -C \
    '1:client name:' \
    '2:target path:_files -/' \
    '--help[Show help message]' \
    '-h[Show help message]'
}

